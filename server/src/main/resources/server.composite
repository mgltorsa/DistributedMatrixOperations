<?xml version="1.0" encoding="UTF-8"?>

<composite xmlns="http://www.osoa.org/xmlns/sca/1.0"
  xmlns:frascati="http://frascati.ow2.org/xmlns/sca/1.1"
  targetNamespace="http://frascati.ow2.org/helloworld-rmi"
  name="ServerRMI">

  

  <component name="server">

	<service name="s">
      <frascati:binding.rmi host="localhost" serviceName="server" port="5001"/>
      <interface.java interface="co.edu.icesi.interfaces.IServer" />      
  	</service>
  	
  	<reference name="broker">
      <frascati:binding.rmi host="192.168.161.23" serviceName="redirecting" port="5555"/>
  	</reference>
  	
    <implementation.java class="co.edu.icesi.Server" />
 	<property name="service">operations</property>
  </component>


</composite>
