<?xml version="1.0" encoding="UTF-8"?>

<composite xmlns="http://www.osoa.org/xmlns/sca/1.0"
  xmlns:frascati="http://frascati.ow2.org/xmlns/sca/1.1"
  targetNamespace="http://frascati.ow2.org/helloworld-rmi"
  name="BrokerRMI">

  <service name="balancer" promote="Broker/redirecting">
      <frascati:binding.rmi host="192.168.161.24" serviceName="redirecting" port="5555"/>
  </service>

  <component name="Broker">
 
    <service name="redirecting">
      <interface.java interface="co.edu.icesi.interfaces.IBalancer" />
    </service>
 
    <implementation.java class="co.edu.icesi.Balancer" />
 
  </component>

 

</composite>
